    "vite": "^6.0.0"
  },
  "type": "module",
  "dependencies": {
    "@appwrite.io/pink-icons": "^0.25.0",
    "@mermaid-js/parser": "^0.6.3",
    "@sveltejs/adapter-node": "^5.2.12",
    "@tailwindcss/vite": "^4.1.3",
